#include <stdio.h>

int main() {
    int values[200];
    int positive = 0, negative = 0, zero = 0;
    
    printf("Enter 200 numbers:\n");
    for (int i = 0; i < 200; i++) {
        scanf("%d", &values[i]);
    
        if (values[i] > 0) {
            positive++;
        } else if (values[i] < 0) {
            negative++;
        } else {
            zero++;
        }
    }

    printf("Positive numbers: %d\n", positive);
    printf("Negative numbers: %d\n", negative);
    printf("Zeroes: %d\n", zero);
    
    return 0;
}

#include <stdio.h>

int main() {
    int n;

    printf("Enter the number of times to print the name (n): ");
    scanf("%d", &n);

    if (n <= 0) {
        printf("Please enter a positive integer.\n");
        return 1;
    }

    for (int i = 0; i < n; i++) {
        printf("prince\n");
    }

    return 0;
}

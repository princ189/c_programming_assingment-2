#include <stdio.h>

int main() {
    int n;

    printf("Enter the number of odd numbers to print (n): ");
    scanf("%d", &n);

    if (n <= 0) {
        printf("Please enter a positive integer.\n");
        return 1;
    }
    printf("The first %d odd numbers are:\n", n);
    for (int i = 1, count = 0; count < n; i += 2) {
        printf("%d ", i);
        count++;
    }
    printf("\n");

    return 0;
}

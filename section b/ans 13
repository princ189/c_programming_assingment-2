#include <stdio.h>

int main() {
    int sum = 0;

    printf("Numbers divisible by 13 from 1 to 100:\n");
    for (int i = 1; i <= 100; i++) {
        if (i % 13 == 0) {
            printf("%d ", i);
            sum += i;
        }
    }
    printf("\nSum of numbers divisible by 13: %d\n", sum);

    return 0;
}
